{
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 13 * * *"
    },
    {
      "name": "artistsIn",
      "type": "blob",
      "path": "azure-webjobs-hosts/artists.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "name": "artistsOut",
      "type": "blob",
      "path": "azure-webjobs-hosts/artists.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}