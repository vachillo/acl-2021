{
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 13 * * 1-5",
      "runOnStartup" : false
    },
    {
      "name": "artistsIn",
      "type": "blob",
      "path": "azure-webjobs-hosts/artists.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "name": "artistsOut",
      "type": "blob",
      "path": "azure-webjobs-hosts/artists.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    },
    {
      "name": "artistsInWeek",
      "type": "blob",
      "path": "azure-webjobs-hosts/artists_week.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "name": "artistsOutWeek",
      "type": "blob",
      "path": "azure-webjobs-hosts/artists_week.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ],
  "disabled": false
}
