{
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 14 * * 6"
    },
    {
      "name": "artistsIn",
      "type": "blob",
      "path": "azure-webjobs-hosts/artistsIn.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "name": "artistsOut",
      "type": "blob",
      "path": "azure-webjobs-hosts/artistsIn.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ],
  "disabled": false
}
