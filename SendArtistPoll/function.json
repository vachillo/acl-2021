{
  "bindings": [
    {
      "name": "myTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 14 * * 6"
    },
    {
      "name": "artistsInWeek",
      "type": "blob",
      "path": "azure-webjobs-hosts/artists_week.json",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "name": "artistsOutWeek",
      "type": "blob",
      "path": "azure-webjobs-hosts/artists_week.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}